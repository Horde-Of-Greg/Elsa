{
    "name": "elsa",
    "version": "0.1.0",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/main.js",
        "dev": "ts-node src/main.ts",
        "tscheck": "tsc --noEmit",
        "db:drop": "ts-node src/tools/db/drop.ts",
        "db:create": "ts-node src/tools/db/create.ts",
        "db:reset": "ts-node src/tools/db/reset.ts",
        "db:psql": "ts-node src/tools/db/psql.ts",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "lint": "eslint \"src/**/*.ts\"",
        "lint:report": "eslint \"src/**/*.ts\" --format json --output-file temp/lint/eslint_report.json",
        "bump": "ts-node src/tools/versioning/bump.ts"
    },
    "dependencies": {
        "discord.js": "^14.14.1",
        "dotenv": "^16.3.1",
        "jiti": "^2.6.1",
        "pg": "^8.16.3",
        "prettier": "^3.7.4",
        "reflect-metadata": "^0.2.2",
        "typeorm": "^0.3.27",
        "zod": "^4.1.12"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@types/node": "^20.10.0",
        "@types/pg": "^8.10.9",
        "@typescript-eslint/eslint-plugin": "^8.49.0",
        "@typescript-eslint/parser": "^8.49.0",
        "eslint": "^9.39.1",
        "eslint-plugin-n": "^17.23.1",
        "eslint-plugin-sonarjs": "^3.0.5",
        "eslint-plugin-unused-imports": "^4.3.0",
        "globals": "^16.5.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.3.2",
        "typescript-eslint": "^8.49.0"
    }
}
